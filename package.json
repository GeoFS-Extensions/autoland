{
  "name": "geofs-plugins",
  "description": "Adding new functionalities to the GeoFS Flight Simulator.",
  "version": "1.0.0",
  "homepage": "https://geofsautoland.wixsite.com/autoland",
  "author": "Nicolas Rodriguez, Guy Adler",
  "license": "GPL-3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeoFS-Autoland/autoland.git"
  },
  "scripts": {
    "docs": "npx jsdoc -c jsdoc.json",
    "tsc": "node build.js && npx tsc && npx rimraf ./build/**/*.ts && node postTsc.js",
    "build": "npm i && npm run tsc && npx prettier --write . && npx eslint . --fix",
    "commit": "python commit.py",
    "test": "npx prettier --check . && npx eslint ."
  },
  "devDependencies": {
    "@types/chrome": "0.0.157",
    "@types/jquery": "3.5.6",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "cws-publish": "2.0.6",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "fs-extra": "10.0.0",
    "glob": "7.1.7",
    "jsdoc": "3.6.7",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "typescript": "4.4.3"
  }
}
